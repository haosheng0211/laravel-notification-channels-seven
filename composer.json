{
    "name": "laravel-notification-channels/seven",
    "description": "Laravel Seven Notification Channel",
    "homepage": "https://github.com/laravel-notification-channels/seven",
    "license": "MIT",
    "authors": [
        {
            "name": "Mr.Jin",
            "email": "haosheng0211@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/http": "~5.5 || ~6.0 || ~7.0 || ~8.0 || ~9.0",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ~8.0 || ~9.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0 || ~9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Seven\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Seven\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Seven\\SevenServiceProvider"
            ]
        }
    }
}
